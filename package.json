{
	"name": "protect-ukraine-now",
	"description": "Protect Ukraine Now",
	"version": "4.0.0",
	"type": "module",
	"packageManager": "yarn@3.5.1",
	"private": true,
	"license": "MIT",
	"homepage": "https://protectukrainenow.org",
	"scripts": {
		"load": "node src/data/load.js",
		"dev": "set VITE_APP_NAME=pun&& vite",
		"dev:pun": "set VITE_APP_NAME=pun&& vite",
		"dev:uat": "set VITE_APP_NAME=uat&& vite",
		"build": "yarn load && set VITE_APP_NAME=pun&& rakkas build && yarn move",
		"build:pun": "yarn load && set VITE_APP_NAME=pun&& rakkas build && yarn move",
		"build:uat": "yarn load && cross-env VITE_APP_NAME='uat' && rakkas build && yarn move",
		"move": "mv dist/server/cloudflare-workers-bundle.js dist/client/_worker.js"
	},
	"devDependencies": {
		"@rakkasjs/eslint-config": "0.6.20",
		"@types/react": "^18.0.28",
		"@types/react-dom": "^18.0.11",
		"@yarnpkg/sdks": "^3.0.0-rc.44",
		"eslint": "^8.34.0",
		"rakkasjs": "0.6.20",
		"typescript": "^4.9.5",
		"vite": "^4.1.3",
		"vite-tsconfig-paths": "^4.0.5",
		"wrangler": "^3.0.0"
	},
	"dependencies": {
		"@hattip/adapter-cloudflare-workers": "^0.0.34",
		"@hattip/adapter-deno": "^0.0.34",
		"clsx": "^1.2.1",
		"country-flag-icons": "^1.5.7",
		"cross-env": "^7.0.3",
		"date-fns": "^2.30.0",
		"markdown-to-jsx": "^7.2.0",
		"netlify-cli": "^15.1.1",
		"rambda": "^7.5.0",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-google-charts": "^4.0.0",
		"react-icons": "^4.8.0",
		"reset-css": "^5.0.1",
		"sass": "^1.62.1"
	},
	"resolutions": {
		"markdown-to-jsx@^7.2.0": "patch:markdown-to-jsx@npm%3A7.2.0#./.yarn/patches/markdown-to-jsx-npm-7.2.0-abe71d582b.patch"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/protect-ukraine-now/preact-netlify.git"
	}
}
