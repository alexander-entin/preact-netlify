{
	"name": "protect-ukraine-now",
	"description": "Protect Ukraine Now",
	"version": "4.0.1",
	"type": "module",
	"packageManager": "yarn@3.5.1",
	"private": true,
	"license": "MIT",
	"homepage": "https://protectukrainenow.org",
	"scripts": {
		"dev": "vite",
		"dev:pun": "cross-env VITE_APP_NAME='pun' vite",
		"dev:uat": "cross-env VITE_APP_NAME='uat' vite",
		"load": "yarn load:sheets && yarn load:sankey",
		"load:sheets": "node src/data/load-sheets.js",
		"load:sankey": "node src/data/load-sankey.js",
		"build": "yarn load && rakkas build",
		"preview": "cross-env NODE_ENV='development' rakkas build && wrangler dev",
		"switch:stage": "sed -i 's/prod/stage/g' wrangler.toml",
		"switch:prod": "sed -i 's/stage/prod/g' wrangler.toml",
		"deploy:stage": "yarn build && wrangler deploy",
		"deploy:prod": "yarn build && yarn switch:prod && wrangler deploy"
	},
	"devDependencies": {
		"@rakkasjs/eslint-config": "0.6.20",
		"@types/react": "^18.2.28",
		"@types/react-dom": "^18.2.13",
		"cross-env": "^7.0.3",
		"eslint": "^8.51.0",
		"typescript": "^5.2.2",
		"vite-tsconfig-paths": "^4.2.1",
		"wrangler": "^3.13.1"
	},
	"dependencies": {
		"@hattip/adapter-cloudflare-workers": "^0.0.35",
		"clsx": "^2.0.0",
		"country-flag-icons": "^1.5.7",
		"date-fns": "^2.30.0",
		"markdown-to-jsx": "^7.3.2",
		"rakkasjs": "^0.6.20",
		"rambda": "^8.5.0",
		"react": "^18.2.0",
		"react-append-head": "^1.0.16",
		"react-dom": "^18.2.0",
		"react-google-charts": "^4.0.1",
		"react-icons": "^4.11.0",
		"reset-css": "^5.0.2",
		"sass": "^1.69.3",
		"vite": "^4.4.11"
	},
	"resolutions": {
		"markdown-to-jsx@^7.2.0": "patch:markdown-to-jsx@npm%3A7.2.0#./.yarn/patches/markdown-to-jsx-npm-7.2.0-abe71d582b.patch"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/protect-ukraine-now/preact-netlify.git"
	}
}
