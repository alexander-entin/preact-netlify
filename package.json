{
  "private": true,
  "name": "protect-ukraine-now",
  "description": "Protect Ukraine Now",
  "version": "0.1.3",
  "license": "MIT",
  "scripts": {
    "start": "per-env",
    "start:production": "npm run -s serve",
    "start:development": "npm run -s dev",
    "build": "preact build --prerenderUrls ./prerender-urls.js",
    "dev": "preact watch --prerender --prerenderUrls ./prerender-urls.js",
    "lint": "eslint src",
    "test": "jest"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "no-unused-vars": "off"
    }
  },
  "eslintIgnore": [
    "build/*"
  ],
  "devDependencies": {
    "eslint": "^8.20.0",
    "eslint-config-synacor": "^2.0.2",
    "eslint-plugin-babel": "^5.3.1",
    "esm": "^3.2.25",
    "glob": "^7.1.4",
    "jest": "^21.2.1",
    "jest-preset-preact": "^1.0.0",
    "markdown": "^0.5.0",
    "per-env": "^1.0.2",
    "preact-cli": "^3.0.1",
    "preact-render-spy": "^1.2.1",
    "react-syntax-highlighter": "^12.2.1",
    "sass": "^1.53.0",
    "sass-loader": "10",
    "webpack-bundle-analyzer": "^3.4.1"
  },
  "dependencies": {
    "@preact/prerender-data-provider": "^3.0.1",
    "classnames": "^2.3.1",
    "date-fns": "^2.28.0",
    "imagemin-mozjpeg": "^8.0.0",
    "imagemin-webpack-plugin": "^2.4.2",
    "markdown-to-jsx": "^6.11.4",
    "parse-md": "^2.0.4",
    "preact": "^10.4.6",
    "preact-cli-plugin-netlify": "^1.6.0",
    "preact-helmet": "^4.0.0-alpha-3",
    "preact-i18n": "^2.4.0-preactx",
    "preact-render-to-string": "^5.1.10",
    "preact-router": "^3.0.1",
    "rambda": "^7.1.4",
    "react-google-charts": "^4.0.0",
    "react-icons": "^4.4.0",
    "react-lite-youtube-embed": "^2.2.2",
    "reset-css": "^5.0.1"
  },
  "jest": {
    "preset": "jest-preset-preact"
  },
  "bugs": {
    "url": "https://t.me/ProtectUkraineNow_tech"
  },
  "homepage": "https://protectukrainenow.org",
  "keywords": [
    "preact",
    "netlify"
